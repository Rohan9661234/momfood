<!DOCTYPE html>
<!-- CodingMaker-->
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title></title>
    <!--CSS Style-->
    <link rel="stylesheet" href="http://localhost:8000///css/login.css" />
    <link
      href="https://fonts.googleapis.com/css?family=Montserrat:400,700"
      rel="stylesheet"
    />
  </head>
  <style>
    body {
      background-color: white;
    }
    .form {
      background: gray;
      box-shadow: inset;
    }

    .inputbox span {
      color: black;
    }
    .form h2 {
      color: black;
    }
    .form h2 {
      color: black;
    }
    input[type="submit"] {
      color: black;
      background: white;
    }
  </style>
  <body>
    <div class="box">
      <form class="form" method="post">
        <h2>Profile Page</h2>
        <div class="inputbox">
          <input
            type="text"
            required="required"
            name="email"
            readonly
            value="<%=name%>"
          />
          <span>UserName </span>
          <i></i>
        </div>

        <div class="inputbox">
          <input
            type="email"
            required="required"
            name="password"
            readonly
            value="<%=email%>"
          />
          <span>Email</span>
          <i></i>
        </div>

        <div class="inputbox">
          <input
            type="number"
            required="required"
            name="phone"
            value="<%=phone%>"
          />
          <span>Phone</span>
          <i></i>
        </div>

        <input type="submit" value="Submit" />
      </form>
    </div>
  </body>
</html>
